<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <ImageView
            android:id="@+id/item_token_property_icon"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            />
        <TextView
            android:id="@+id/item_token_property_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/item_token_property_icon"
            android:layout_marginLeft="@dimen/dp_15"
            android:textSize="@dimen/dp_14"
            android:textColor="#181818"
            />
        <TextView
            android:id="@+id/item_token_property_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/default_text"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_11"
            android:layout_marginRight="@dimen/dp_15"
            />
        <TextView
            android:id="@+id/item_token_property_to_rmb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/dp_14"
            android:layout_alignParentRight="true"
            android:layout_below="@id/item_token_property_count"
            android:layout_marginRight="@dimen/dp_15"
            />
    </RelativeLayout>
</RelativeLayout>