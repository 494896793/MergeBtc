package com.bochat.app.business.main.dynamic;

import com.bochat.app.common.contract.dynamic.ProtocolBookContract;
import com.bochat.app.mvp.presenter.BasePresenter;

import static com.bochat.app.app.RouteExtra.ProtocolBookEntity;

/**
 * 2019/5/29
 * Author LDL
 **/
public class ProtocolBookPresenter extends BasePresenter<ProtocolBookContract.View> implements ProtocolBookContract.Presenter {


    @Override
    public void initInjector() {
        getBusinessComponent().inject(this);
    }

    @Override
    public void onViewRefresh() {
        super.onViewRefresh();
        if(getView().getViewIntent().hasExtra(ProtocolBookEntity)){
            try{
                com.bochat.app.model.bean.ProtocolBookEntity entity= (com.bochat.app.model.bean.ProtocolBookEntity) getView().getViewIntent().getSerializableExtra(ProtocolBookEntity);
                getView().refreshData(entity);
            }catch (Exception e){
                e.printStackTrace();
            }
        }
    }
}
