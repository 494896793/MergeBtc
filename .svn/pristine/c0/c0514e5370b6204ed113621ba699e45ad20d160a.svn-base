<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.flyco.tablayout.SlidingTabLayout
        android:id="@+id/market_quotation_entrust_pager_tab"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_30"
        android:background="#FBFAFA"
        app:tl_indicator_color="@color/transparent"
        app:tl_indicator_corner_radius="@dimen/dp_2"
        app:tl_indicator_height="@dimen/dp_2"
        app:tl_indicator_width="@dimen/dp_14"
        app:tl_tab_space_equal="true"
        app:tl_textSelectColor="#0084FF"
        app:tl_textUnselectColor="@color/color_999999"
        app:tl_textsize="@dimen/sp_12" />

    <android.support.v4.view.ViewPager
        android:id="@+id/market_quotation_entrust_pager_view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>