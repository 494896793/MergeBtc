public class DynamicAdapterEntity {

    private DynamicTopShopListEntity dynamicTopShopListEntity;

    private DynamicPlushEntity dynamicPlushEntity;

    private DynamicBannerListEntity dynamicBannerListEntity;

    private List<DynamicAdapterBannerEntity> bannerEntityList;

    private boolean viewLineVisible=true;

    public List<DynamicAdapterBannerEntity> getBannerEntityList() {
        return bannerEntityList;
    }

    public void setBannerEntityList(List<DynamicAdapterBannerEntity> bannerEntityList) {
        this.bannerEntityList = bannerEntityList;
    }

    public boolean isViewLineVisible() {
        return viewLineVisible;
    }

    public void setViewLineVisible(boolean viewLineVisible) {
        this.viewLineVisible = viewLineVisible;
    }

    public DynamicPlushEntity getDynamicPlushEntity() {
        return dynamicPlushEntity;
    }

    public void setDynamicPlushEntity(DynamicPlushEntity dynamicPlushEntity) {
        this.dynamicPlushEntity = dynamicPlushEntity;
    }

    private String title;

    private int type;

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }


    public DynamicBannerListEntity getDynamicBannerListEntity() {
        return dynamicBannerListEntity;
    }

    public void setDynamicBannerListEntity(DynamicBannerListEntity dynamicBannerListEntity) {
        this.dynamicBannerListEntity = dynamicBannerListEntity;
    }

    public DynamicTopShopListEntity getDynamicTopShopListEntity() {
        return dynamicTopShopListEntity;
    }

    public void setDynamicTopShopListEntity(DynamicTopShopListEntity dynamicTopShopListEntity) {
        this.dynamicTopShopListEntity = dynamicTopShopListEntity;
    }



    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }
}
