<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_64"
    android:background="#00000000"
    tools:ignore="MissingDefaultResource">
    <RelativeLayout
        android:layout_marginTop="@dimen/dp_20"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        
        <RelativeLayout
            android:id="@+id/top_bar_back_layout"
            android:layout_width="@dimen/dp_50"
            android:layout_height="match_parent"
            android:visibility="invisible">
            <TextView
                android:id="@+id/top_bar_back_btn"
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_8"
                android:background="@mipmap/ic_top_bar_back"
                android:scaleType="fitXY"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/top_bar_title"
            android:layout_width="@dimen/dp_200"
            android:gravity="center_horizontal"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="BoChat"
            android:textColor="#181818"
            android:textSize="@dimen/dp_18"
            android:textStyle="bold"
            />
        <Button
            android:id="@+id/top_bar_ext_btn_2"
            android:layout_width="@dimen/dp_19"
            android:layout_height="@dimen/dp_19"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_20"
            tools:background="@mipmap/ic_chat_setting"
            android:layout_alignParentEnd="true"
            android:visibility="invisible"
            android:scaleType="fitXY"/>
        <Button
            android:id="@+id/top_bar_ext_btn"
            android:layout_width="@dimen/dp_19"
            android:layout_height="@dimen/dp_19"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_20"
            android:layout_toStartOf="@id/top_bar_ext_btn_2"
            tools:background="@mipmap/ic_chat_shandui"
            android:visibility="invisible"
            android:scaleType="fitXY"/>
        <TextView
            android:id="@+id/top_bar_ext_btn_3"
            android:layout_width="wrap_content"
            android:minWidth="@dimen/dp_60"
            android:layout_height="@dimen/dp_25"
            android:text="设置"
            android:textColor="#181818"
            android:visibility="invisible"
            android:textSize="@dimen/dp_16"
            android:gravity="center"
            android:textStyle="normal"
            android:focusable="true"
            android:clickable="true"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"/>
    </RelativeLayout>
</RelativeLayout>