<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#F2F2F2"
    android:orientation="vertical">
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_95"
        android:background="#FFFFFF">

        <TextView
            android:id="@+id/item_mine_order_first_title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_15"
            android:text="想兑换"
            android:textColor="#181818"
            android:textSize="@dimen/dp_16" />

        <TextView
            android:id="@+id/item_mine_order_first_count"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_marginLeft="@dimen/dp_78"
            android:layout_marginTop="@dimen/dp_15"
            android:text="1.000000BX"
            android:textColor="#181818"
            android:textSize="@dimen/dp_16" />


        <TextView
            android:id="@+id/item_mine_order_second_title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_below="@+id/item_mine_order_first_title"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_20"
            android:text="支付"
            android:textColor="#181818"
            android:textSize="@dimen/dp_16" />


        <TextView
            android:id="@+id/item_mine_order_second_count"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_below="@+id/item_mine_order_first_title"
            android:layout_marginLeft="@dimen/dp_78"
            android:layout_marginTop="@dimen/dp_21"
            android:text="2.000000USDT"
            android:textColor="#ff181818"
            android:textSize="@dimen/dp_16" />


        <TextView
            android:id="@+id/item_mine_order_status"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:text="已完成"
            android:textColor="#999999"
            android:textSize="@dimen/dp_16" />

    </RelativeLayout>

</LinearLayout>