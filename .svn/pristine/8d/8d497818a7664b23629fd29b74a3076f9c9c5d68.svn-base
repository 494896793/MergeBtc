package com.bochat.app.business.main.conversation;

import com.alibaba.android.arouter.launcher.ARouter;
import com.bochat.app.app.RouteExtra;
import com.bochat.app.app.RouteTable;
import com.bochat.app.common.contract.conversation.ConversationContract;
import com.bochat.app.mvp.presenter.BasePresenter;

/**
 * Author      : FJ
 * CreateDate  : 2019/04/15 20:14
 * Description :
 */

public class ConversationPresenter extends BasePresenter<ConversationContract.View> implements ConversationContract.Presenter{
    
    @Override
    public void initInjector() {
        getBusinessComponent().inject(this);
    }

    @Override
    public void onSearchFriendBtnClick() {
        ARouter.getInstance().build(RouteTable.CONVERSATION_ADD_FRIEND).navigation();
    }

    @Override
    public void onCreateGroupBtnClick() {
        ARouter.getInstance().build(RouteTable.CONVERSATION_SELECT_FRIEND).navigation();
    }

    @Override
    public void onSearchHistoryClick() {
        ARouter.getInstance().build(RouteTable.CONVERSATION_SEARCH_CONVERSATION).navigation();
    }

    @Override
    public void onQRScanClick() {
        ARouter.getInstance().build(RouteTable.SCAN_QR_CODE).withString(RouteExtra.ReturnUrl,
                RouteTable.BOOK_ADDRESS_FRIEND_DETAIL).navigation();
    }
}
