<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/default_layout"
    android:orientation="vertical">

    <com.bochat.app.app.view.BoChatTopBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:top_titleText="设置备注"
        app:top_hasBackBtn="true"/>
    
    <EditText
        android:id="@+id/cv_group_memeber_manage_note_input"
        android:layout_width="match_parent"
        android:hint="备注名"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_10"
        android:background="#FFFFFF"
        android:maxLines="1"
        android:layout_height="@dimen/dp_44"/>
    
    <Button
        android:id="@+id/cv_group_memeber_manage_note_enter_btn"
        android:layout_width="@dimen/dp_345"
        android:layout_height="@dimen/dp_44"
        android:layout_marginLeft="@dimen/dp_15"
        style="@style/default_btn"
        android:text="提交"
        android:layout_marginTop="@dimen/dp_60"
        />
    
</LinearLayout>