<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/default_layout">

    <com.bochat.app.app.view.BoChatTopBar
        android:id="@+id/cv_group_member_manage_top_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:top_hasBackBtn="true"
        app:top_titleText="设置"/>

    <com.bochat.app.app.view.BoChatItemView
        android:id="@+id/cv_group_member_manage_note"
        android:layout_below="@id/cv_group_member_manage_top_bar"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_44"
        app:item_text="备注"
        android:layout_marginTop="@dimen/dp_11"/>
    <com.bochat.app.app.view.BoChatItemView
        android:id="@+id/cv_group_member_manage_history"
        android:layout_below="@id/cv_group_member_manage_note"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_44"
        app:item_text="聊天记录"
        />
    <com.bochat.app.app.view.BoChatItemView
        android:id="@+id/cv_group_member_manage_forbidden"
        android:layout_below="@id/cv_group_member_manage_history"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_44"
        android:layout_marginTop="@dimen/dp_10"
        app:item_hasRightBtn="false"
        app:item_hasSwitchBtn="true"
        app:item_text="禁言"
        />
    <com.bochat.app.app.view.BoChatItemView
        android:id="@+id/cv_group_member_manage_black"
        android:layout_below="@id/cv_group_member_manage_forbidden"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_44"
        app:item_hasRightBtn="false"
        app:item_hasSwitchBtn="true"
        app:item_text="拉黑此人"
        />

    <Button
        android:id="@+id/cv_group_member_manage_enter_btn"
        android:layout_width="@dimen/dp_345"
        android:layout_height="@dimen/dp_44"
        android:layout_marginLeft="@dimen/dp_15"
        android:background="@drawable/shape_white_btn"
        android:text="请出群聊"
        android:textColor="#FE695E"
        android:textSize="@dimen/dp_17"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/dp_30"
        />
    
</RelativeLayout>