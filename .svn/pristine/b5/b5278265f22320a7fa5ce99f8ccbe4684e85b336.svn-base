<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.bochat.app.app.view.BoChatTopBar
        android:id="@+id/bochat_topbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:top_titleText="消息"
        app:top_hasExtBtn="true"
        app:top_hasExt2Btn="true"
        app:top_background="#fff2f2f2"
        app:top_extButtonSrc="@mipmap/ic_bochat_scan"
        app:top_extButton2Src="@mipmap/ic_bochat_add"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v7.widget.RecyclerView>

</LinearLayout>