package com.bochat.app.business.main.conversation;

import com.alibaba.android.arouter.launcher.ARouter;
import com.bochat.app.app.RouteTable;
import com.bochat.app.common.contract.conversation.AddGroupChatContract;
import com.bochat.app.model.bean.GroupEntity;
import com.bochat.app.mvp.presenter.BasePresenter;

/**
 * Author      : FJ
 * CreateDate  : 2019/04/16 11:35
 * Description :
 */

public class AddGroupChatPresenter extends BasePresenter<AddGroupChatContract.View> implements AddGroupChatContract.Presenter {
   
    @Override
    public void onCreateBtnClick() {
        ARouter.getInstance().build(RouteTable.CONVERSATION_SELECT_FRIEND).navigation();
    }

    @Override
    public void onRecommendItemClick(GroupEntity item) {
        ARouter.getInstance().build(RouteTable.CONVERSATION_GROUP_DETAIL).navigation();
    }

    @Override
    public void onSearchItemClick(GroupEntity item) {
        ARouter.getInstance().build(RouteTable.CONVERSATION_GROUP_DETAIL).navigation();
    }

    @Override
    public void onSearchTextUpdate(String text) {
        if(isActive()){
            getView().showTips("onSearchTextUpdate " + text);
        }
    }

    @Override
    public void initInjector() {
        getBusinessComponent().inject(this);
    }
}
