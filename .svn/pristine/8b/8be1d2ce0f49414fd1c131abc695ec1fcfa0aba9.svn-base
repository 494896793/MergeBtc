package com.bochat.app.model.bean;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 2019/5/5
 * Author LDL
 **/
public class RedPacketPeopleEntity extends CodeEntity implements Serializable {

    private int receiver_id;
    private int reward_id;
    private String receiver_name;
    private BigDecimal receive_money;

    @Override
    public String toString() {
        return "RedPacketPeopleEntity{" +
                "receiver_id=" + receiver_id +
                ", reward_id=" + reward_id +
                ", receiver_name='" + receiver_name + '\'' +
                ", receive_money=" + receive_money +
                '}';
    }

    public int getReceiver_id() {
        return receiver_id;
    }

    public void setReceiver_id(int receiver_id) {
        this.receiver_id = receiver_id;
    }

    public int getReward_id() {
        return reward_id;
    }

    public void setReward_id(int reward_id) {
        this.reward_id = reward_id;
    }

    public String getReceiver_name() {
        return receiver_name;
    }

    public void setReceiver_name(String receiver_name) {
        this.receiver_name = receiver_name;
    }

    public BigDecimal getReceive_money() {
        return receive_money;
    }

    public void setReceive_money(BigDecimal receive_money) {
        this.receive_money = receive_money;
    }
}
