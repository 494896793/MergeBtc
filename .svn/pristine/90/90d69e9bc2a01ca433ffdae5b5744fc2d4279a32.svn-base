<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
    android:background="#FFFFFF">

    <com.bochat.app.app.view.BoChatTopBar
        android:id="@+id/mine_property_recharge_result_top_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:top_titleText="充值成功"
        app:top_hasBackBtn="true"
        android:background="#f2f2f2"/>
    
    <ImageView
        android:layout_width="@dimen/dp_120"
        android:layout_height="@dimen/dp_80"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_90"
        android:src="@mipmap/ic_mine_money_success" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_24"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_15"
        android:text="充值成功"
        android:textColor="#ff181818"
        android:textSize="@dimen/dp_17" />


    <Button
        android:id="@+id/mine_property_recharge_result_enter_btn"
        android:layout_width="@dimen/dp_250"
        android:layout_height="@dimen/dp_44"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_92"
        android:background="@drawable/shape_login_btn"
        android:text="完成"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/dp_17" />

</LinearLayout>