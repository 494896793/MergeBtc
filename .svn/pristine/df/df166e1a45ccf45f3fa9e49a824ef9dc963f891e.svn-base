<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.bochat.app.app.view.BoChatTopBar
        android:visibility="gone"
        android:id="@+id/bochat_topbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:top_titleText="通讯录"
        app:top_background="@color/white"
        />


    <android.support.design.widget.TabLayout
        android:paddingTop="@dimen/dp_10"
        android:background="#ffffff"
        android:id="@+id/tableayout"
        android:layout_below="@id/bochat_topbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"></android.support.design.widget.TabLayout>

    <android.support.v4.view.ViewPager
        android:layout_below="@id/tableayout"
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v4.view.ViewPager>

    <RelativeLayout
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_10"
        android:paddingTop="@dimen/dp_17"
        android:id="@+id/back_relative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:src="@mipmap/ic_top_bar_back"
            android:layout_width="@dimen/dp_17"
            android:layout_height="@dimen/dp_30" />
    </RelativeLayout>

</RelativeLayout>