package com.bochat.app.model.bean;

import java.io.Serializable;

/**
 * 2019/5/23
 * Author LDL
 **/
public class ViewSpotItemEntity extends CodeEntity implements Serializable {

    private int id;
    private String title;
    private String showImage;
    private String content;
    private int type;       //1-小缩略图展示；2-大缩略图展示；3-多缩略图展示
    private String upTime;
    private int ifRecommend;
    private String comeForm;

    @Override
    public String toString() {
        return "ViewSpotItemEntity{" +
                "id=" + id +
                ", title='" + title + '\'' +
                ", showImage='" + showImage + '\'' +
                ", content='" + content + '\'' +
                ", type=" + type +
                ", upTime='" + upTime + '\'' +
                ", ifRecommend=" + ifRecommend +
                ", comeForm='" + comeForm + '\'' +
                '}';
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getShowImage() {
        return showImage;
    }

    public void setShowImage(String showImage) {
        this.showImage = showImage;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public String getUpTime() {
        return upTime;
    }

    public void setUpTime(String upTime) {
        this.upTime = upTime;
    }

    public int getIfRecommend() {
        return ifRecommend;
    }

    public void setIfRecommend(int ifRecommend) {
        this.ifRecommend = ifRecommend;
    }

    public String getComeForm() {
        return comeForm;
    }

    public void setComeForm(String comeForm) {
        this.comeForm = comeForm;
    }
}
