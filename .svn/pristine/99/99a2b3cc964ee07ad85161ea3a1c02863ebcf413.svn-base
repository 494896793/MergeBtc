<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <com.bochat.app.app.view.BoChatTopBar
        android:id="@+id/bochat_topbar"
        app:top_background="@color/white"
        app:top_extButton2Src="@mipmap/ic_bochat_add"
        app:top_extButtonSrc="@null"
        app:top_hasExt2Btn="true"
        app:top_hasExtBtn="false"
        app:top_titleText="消息"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:id="@+id/bochat_search_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_36"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@drawable/shape_search"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/bochat_search_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@mipmap/ic_bochat_search"
            android:drawablePadding="@dimen/dp_6"
            android:text="@string/conversation_search_str"
            android:textAlignment="center"
            android:textColor="@color/colorFontColorForDark"
            android:textSize="@dimen/dp_16"/>

    </LinearLayout>

    <!--<RelativeLayout-->
        <!--android:id="@+id/bochat_dialog_layout"-->
        <!--android:layout_width="1dp"-->
        <!--android:layout_height="1dp"-->
        <!--android:layout_marginTop="@dimen/dp_48">-->
    <!--</RelativeLayout>-->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_11">
        <FrameLayout
            android:id="@+id/conversationlist"
            android:name="io.rong.imkit.fragment.ConversationListFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </RelativeLayout>
</LinearLayout>