<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.bochat.app.app.view.BoChatTopBar
        android:id="@+id/list_app_topbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F2F2F2"
        app:top_titleText="添加朋友"
        app:top_hasBackBtn="true"/>
    <android.support.design.widget.TabLayout
        android:background="#ffffff"
        android:id="@+id/tableayout"
        android:layout_below="@id/top_list_linear2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"></android.support.design.widget.TabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager"
        android:layout_below="@id/tableayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>
</LinearLayout>