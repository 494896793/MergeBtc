package com.bochat.app.model.bean;

import java.io.Serializable;

public class MarketCurrency extends CodeEntity implements Serializable {
    private int marketId;
    private int sellerCurrencyId;
    private String sellerCurrencyName;
    private int buyerCurrencyId;
    private String buyerCurrencyName;
    private String buyerCurrencyImage;

    public int getMarketId() {
        return marketId;
    }

    public void setMarketId(int marketId) {
        this.marketId = marketId;
    }

    public int getSellerCurrencyId() {
        return sellerCurrencyId;
    }

    public void setSellerCurrencyId(int sellerCurrencyId) {
        this.sellerCurrencyId = sellerCurrencyId;
    }

    public String getSellerCurrencyName() {
        return sellerCurrencyName;
    }

    public void setSellerCurrencyName(String sellerCurrencyName) {
        this.sellerCurrencyName = sellerCurrencyName;
    }

    public int getBuyerCurrencyId() {
        return buyerCurrencyId;
    }

    public void setBuyerCurrencyId(int buyerCurrencyId) {
        this.buyerCurrencyId = buyerCurrencyId;
    }

    public String getBuyerCurrencyName() {
        return buyerCurrencyName;
    }

    public void setBuyerCurrencyName(String buyerCurrencyName) {
        this.buyerCurrencyName = buyerCurrencyName;
    }

    public String getBuyerCurrencyImage() {
        return buyerCurrencyImage;
    }

    public void setBuyerCurrencyImage(String buyerCurrencyImage) {
        this.buyerCurrencyImage = buyerCurrencyImage;
    }

    @Override
    public String toString() {
        return "MarketCurrency{" +
                "marketId=" + marketId +
                ", sellerCurrencyId=" + sellerCurrencyId +
                ", sellerCurrencyName='" + sellerCurrencyName + '\'' +
                ", buyerCurrencyId=" + buyerCurrencyId +
                ", buyerCurrencyName='" + buyerCurrencyName + '\'' +
                ", buyerCurrencyImage='" + buyerCurrencyImage + '\'' +
                '}';
    }
}
